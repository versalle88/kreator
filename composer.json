{
    "name": "versalle/kreator",
    "description": "A simple, PSR-compliant PHP ADR micro-framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Versalle",
            "email": "andrew.versalle@gmail.com"
        }
    ],
    "require": {
        "php": "7.2.* || 7.3.* || 7.4.*",
        "ext-mbstring": "*",
        "guzzlehttp/psr7": "^1.6",
        "nikic/fast-route": "^1.3",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "tracy/tracy": "^2.7",
        "twig/twig": "^3.0",
        "versalle/container": "^1.1"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Versalle\\Framework\\": "src/",
            "App\\": "app/modules/"
        }
    },
    "autoload-dev": {
    },
    "config": {
        "platform": {
            "php": "7.2.24"
        },
        "sort-packages": true
    }
}
